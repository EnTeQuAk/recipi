    "type": "git",
    "url": "ssh://git@github.com/EnTeQuAk/recipi.git"
  },
  "private": true,
  "dependencies": {
    "6to5ify": "^3.1.2",
    "bower": "^1.3.12",
    "brace": "^0.4.0",
    "browserify": "^3.44.2",
    "browserify-shim": "^3.4.1",
    "del": "^1.1.1",
    "gulp": "^3.8.10",
    "gulp-jscs": "^1.4.0",
    "gulp-jshint": "^1.9.0",
    "gulp-lintspaces": "^0.2.2",
    "gulp-livereload": "^3.4.0",
    "gulp-load-plugins": "^0.8.0",
    "gulp-plumber": "^0.6.6",
    "gulp-sass": "^1.3.2",
    "gulp-shell": "^0.2.11",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-watch": "^3.0.0",
    "tiny-lr": "^0.1.4"
  },
  "scripts": {
    "postinstall": "bower install"
  },
  "browser": {
    "modernizr": "./src/recipi/static/js/vendor/modernizr.js",
    "foundation": "./bower_components/foundation/js/foundation.js",
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "backbone": "./bower_components/backbone/backbone.js"
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "modernizr": {
      "exports": "Modernizr"
    },
    "foundation": {
      "exports": "Foundation",
      "depends": [
        "jquery:$"
      ]
    },
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "devDependencies": {
    "6to5ify": "^3.1.2",
    "browserify": "^3.46.1",
    "gulp-browserify": "^0.5.1",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0"
  }
}
